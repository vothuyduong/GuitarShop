<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>
	<rule>
		<from>/dang-nhap$</from>
		<to>/login</to>
	</rule>
	<rule>
		<from>/dang-xuat$</from>
		<to>/logout</to>
	</rule>
	<rule>
		<from>/dang-ki$</from>
		<to>/register</to>
	</rule>
	<rule>
		<from>/lien-he$</from>
		<to>/contact</to>
	</rule>
	<rule>
		<from>/gio-hang/(.+)-([0-9]+)$</from>
		<to>/cart?id=$2</to>
	</rule>
	<rule>
		<from>/gio-hang$</from>
		<to>/cart</to>
	</rule>
	<rule>
		<from>/(.+)-([0-9]+)$</from>
		<to>/cat?id=$2</to>
	</rule>
	<rule>
		<from>/(.+)/(.+)-([0-9]+).html$</from>
		<to>/detail?id=$3</to>
	</rule>
</urlrewrite>

